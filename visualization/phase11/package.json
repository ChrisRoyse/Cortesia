{
  "name": "@llmkg/visualization-complete",
  "version": "2.0.0",
  "description": "Complete LLMKG Visualization System - Production Ready Dashboard",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "serve": "npm run build && npm run preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "start": "npm run dev",
    "clean": "rimraf dist node_modules/.vite",
    "postinstall": "npm run type-check"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-redux": "^8.0.5",
    "@reduxjs/toolkit": "^1.9.3",
    "antd": "^5.3.0",
    "@ant-design/icons": "^5.0.1",
    "d3": "^7.8.2",
    "@types/d3": "^7.4.0",
    "three": "^0.150.1",
    "@types/three": "^0.149.0",
    "@react-three/fiber": "^8.11.1",
    "@react-three/drei": "^9.56.24",
    "framer-motion": "^10.0.1",
    "recharts": "^2.5.0",
    "@monaco-editor/react": "^4.4.6",
    "monaco-editor": "^0.36.1",
    "react-markdown": "^8.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "@types/react-syntax-highlighter": "^15.5.6",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.191",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.1",
    "date-fns": "^2.29.3",
    "react-intersection-observer": "^9.4.2",
    "react-window": "^1.8.8",
    "@types/react-window": "^1.8.5",
    "react-virtualized-auto-sizer": "^1.0.20",
    "use-debounce": "^9.0.3",
    "immer": "^9.0.19",
    "reselect": "^4.1.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.4",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.14.4",
    "typescript": "^4.9.5",
    "eslint": "^8.35.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^2.8.4",
    "vitest": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "jsdom": "^21.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "rimraf": "^4.1.2",
    "cross-env": "^7.0.3",
    "concurrently": "^7.6.0"
  },
  "keywords": [
    "llmkg",
    "visualization",
    "brain-inspired",
    "cognitive-architecture",
    "dashboard",
    "real-time",
    "monitoring",
    "performance",
    "production-ready"
  ],
  "author": "LLMKG Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/llmkg/visualization.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}