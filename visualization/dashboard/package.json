{
  "name": "llmkg-dashboard",
  "version": "2.0.0",
  "description": "Real-time visualization dashboard for Large Language Model Knowledge Graphs",
  "private": true,
  "homepage": ".",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "node scripts/start-servers.js",
    "dev:safe": "node scripts/start-servers-safe.js",
    "dev:force": "node scripts/start-servers.js",
    "dev:frontend-only": "vite",
    "dev:fix": "vite --config vite.config.dev.ts",
    "dev:with-concurrently": "concurrently --kill-others-on-fail --prefix-colors \"bgBlue.bold,bgMagenta.bold\" --names \"BACKEND,FRONTEND\" \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "npm run build:backend && npm run serve:backend",
    "start:frontend": "wait-on http://localhost:8090 http://localhost:3001 tcp:8081 --timeout 60000 && vite --host",
    "build:backend": "cd ../.. && cargo build --bin llmkg_api_server",
    "serve:backend": "cd ../.. && cargo run --bin llmkg_api_server",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx bundle-analyzer dist/assets/*.js",
    "serve": "serve -s dist -l 3000",
    "clean": "rm -rf dist node_modules/.cache",
    "check:servers": "wait-on http://localhost:8080 http://localhost:3001 tcp:8081 --timeout 5000",
    "cypress:open": "cypress open --browser electron",
    "cypress:run": "cypress run --browser electron",
    "cypress:run:headed": "cypress run --browser electron --headed",
    "cypress:run:phase1": "cypress run --browser electron --spec \"cypress/e2e/phase1-foundation/**/*.cy.{js,ts}\"",
    "cypress:run:phase2": "cypress run --browser electron --spec \"cypress/e2e/phase2-components/**/*.cy.{js,ts}\"",
    "cypress:run:phase3": "cypress run --browser electron --spec \"cypress/e2e/phase3-realtime/**/*.cy.{js,ts}\"",
    "cypress:run:phase4": "cypress run --browser electron --spec \"cypress/e2e/phase4-visualizations/**/*.cy.{js,ts}\"",
    "cypress:run:phase5": "cypress run --browser electron --spec \"cypress/e2e/phase5-performance/**/*.cy.{js,ts}\"",
    "cypress:run:phase6": "cypress run --browser electron --spec \"cypress/e2e/phase6-error-handling/**/*.cy.{js,ts}\"",
    "cypress:run:phase7": "cypress run --browser electron --spec \"cypress/e2e/phase7-e2e-workflows/**/*.cy.{js,ts}\"",
    "cypress:run:all": "npm run cypress:run:phase1 && npm run cypress:run:phase2 && npm run cypress:run:phase3 && npm run cypress:run:phase4 && npm run cypress:run:phase5 && npm run cypress:run:phase6 && npm run cypress:run:phase7",
    "test:e2e": "npm run cypress:run",
    "test:e2e:headed": "npm run cypress:run:headed",
    "test:full": "npm run test && npm run cypress:run:all",
    "postinstall": "echo 'âœ… Dependencies installed. Run npm run dev to start all servers.'"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@react-three/drei": "^9.88.13",
    "@react-three/fiber": "^8.15.11",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/d3": "^7.4.3",
    "@types/three": "^0.155.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "antd": "^5.12.8",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "diff": "^5.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.16",
    "json-schema": "^0.4.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.4",
    "react-redux": "^9.0.4",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.20.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^3.1.0",
    "rxjs": "^7.8.1",
    "styled-jsx": "^5.1.2",
    "three": "^0.155.0",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.5",
    "@types/diff": "^5.0.9",
    "@types/file-saver": "^2.0.7",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "cypress": "^14.5.2",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nyc": "^17.1.0",
    "serve": "^14.2.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "wait-on": "^7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "react-hooks/exhaustive-deps": "warn"
    }
  }
}
